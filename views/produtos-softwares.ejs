  <section class="container col-xxl-8 bg-light rounded-3 shadow px-4 py-lg-3 mb-4" id="sentinela">
    <div class="row flex-lg-row-reverse align-items-center overflow-hidden g-5 py-5">
      <div class="col-10 col-sm-8 col-lg-4 offset-lg-1 overflow-hidden shadow-lg flex-grow-1 ms-2 p-0"
